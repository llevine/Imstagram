<div class="members">
	<h1>All Photos</h1>
	<h2>
		<a href="/photos/new" id="new">New Post</a>
	</h2>
		<div id="group">
			<% @photos.each do |photo| %>
				<div class="photo">
					<a href="/photos/<%= photo.id %>">
						<img src="<%= photo.img_url %>">
						</a>
				</div>
			<% end %>
		</div>
</div>




