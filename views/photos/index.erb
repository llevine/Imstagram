<h1>Photos</h1>
<a href="/photos/new">New Post</a>
<div class="main_content">
	<% @photos.each do |photo| %>
	<div>
		<h4>TEMPORARILY KEEP THE ID: <%= photo.id %></h4>
		<a href="/photos/<%= photo.id %>"><img src="<%= photo.img_url %>" class="single_photo"></a>
	</div>
	<% end %>
</div>




