<div class="members tags">
	<h1>Click A Tag To See All the Related Images</h1>
	<p>
		<div id="tags_group">
		<% @tags.each do |tag| %>
			<span class="divider"> | </span><a href="/tags/<%= tag.id %>"><%= tag.name %></a><span class="divider"> | </span>
		<% end %>
		</div>
	</p>
</div>


